    </div>
  </div>
{% endblock %}

<footer class="footer-top" role="contentinfo">
  <div class="{{ container }}">
    <div class="row">
      <div class="col-sm-3">
        {% if page.footer_top_1 %}
          {% block footer_top_1 %}
              {{ page.footer_top_1 }}
          {% endblock %}
        {% endif %}
      </div>
      
      <div class="col-sm-3">
